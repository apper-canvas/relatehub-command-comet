          }
        ],
        "operator": "OR"
      },
      "redirectOnDeny": "/login"
    }
  },
"/quotes": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to view quotes",
            "rule": "authenticated"
          }
        ],
        "operator": "OR"
      },
      "redirectOnDeny": "/login"
    }
  },
  "/sales-orders": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to view sales orders",
            "rule": "authenticated"
          }
        ],
        "operator": "OR"
      },
      "redirectOnDeny": "/login"
  },
  "/signup": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Signup page is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/callback": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Callback page is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/error": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Error page is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/reset-password/:appId/:fields": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Reset password page is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/prompt-password/:appId/:emailAddress/:provider": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Prompt password page is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  }
